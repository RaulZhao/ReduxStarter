{
  "name": "redux-starter",
  "version": "1.0.0",
  "description": "simple starter package for react with redux and babel",
  "main": "index.js",
  "scripts": {
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "sass": "sass --watch src/stylesheets/main.scss:dist/stylesheets/main.css",
    "build": "webpack --config webpack-production.config.js --progress --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/masonshin/ReduxStarter.git"
  },
  "keywords": [
    "react",
    "redux",
    "babel",
    "webpack",
    "starter"
  ],
  "author": "Mason Shin",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/masonshin/ReduxStarter/issues"
  },
  "homepage": "https://gitlab.com/masonshin/ReduxStarter#README",
  "dependencies": {
    "babel-preset-stage-1": "^6.13.0",
    "lodash": "^4.15.0",
    "material-ui": "^0.15.4",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "chai": "^3.5.0",
    "chai-jquery": "^2.0.0",
    "css-loader": "^0.25.0",
    "jquery": "^3.1.0",
    "jsdom": "^9.5.0",
    "mocha": "^3.0.2",
    "node-sass": "^3.10.0",
    "react-addons-test-utils": "^15.3.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.2"
  }
}
